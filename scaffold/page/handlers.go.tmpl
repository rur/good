package page

import (
	"net/http"

	"github.com/rur/treetop"
	"<< .Namespace >>/app"
)

// BaseHandler is the root handler use for most pages
func BaseHandler(env *app.Env, rsp treetop.Response, req *http.Request) interface{} {
	return struct {
		Content     interface{}
	}{
		Content:     rsp.HandleSubView("content", req),
	}
}
