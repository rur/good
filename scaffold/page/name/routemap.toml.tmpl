_namespace = "[# .Namespace #]"
_ref = "[# .Name #]"
_template = "[# .PageEntry.Template #]"
_handler = "[# .PageEntry.Handler #]"
_doc = "Base HTML template for [# .Name #] page"
_uri = "/[# .Name #]"

    [[site-nav]]
    _ref = "main-nav"
    _default = true
    _template = "page/templates/nav.html.tmpl"
    _handler = "env.Bind(page.SiteNavHandler)"

    [[content]]
    _ref = "[# .Name #]-placeholder"
    _default = true
    _doc = "Placeholder page"
    _path = "/[# .Name #]"
    _template = "[# .Templates #]/placedholder.html.tmpl"
    _handler = "env.Bind(bindResources(placedholderHandler))"

    [[scripts]]
    _ref = "site-scripts"
    _default = true
    _template = "page/templates/scripts.html.tmpl"
    _handler = "treetop.Noop"
